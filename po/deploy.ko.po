"            ├── mysql\n"
"                ├── seahub.mysql\n"
"                ├── seafile.mysql\n"
"                ├── ccnet.mysql\n"
"```"
msgstr ""
"```sh\n"
"seafile-server-1.6.0\n"
"├── seafile\n"
"├── seahub\n"
"├── upgrade\n"
"    ├── sql\n"
"        ├── 1.6.0\n"
"            ├── mysql\n"
"                ├── seahub.mysql\n"
"                ├── seafile.mysql\n"
"                ├── ccnet.mysql\n"
