"└── seahub_settings.py\n"
"└── ccnet/\n"
"    └── ccnet.conf\n"
"└── seafile/\n"
"    └── seafile.conf\n"
"└── conf/\n"
"    └── seafdav.conf\n"
"└── pro-data/\n"
"    └── seafevents.conf # (전문가판 전용)\n"
"└── seafile-server-latest/\n"
