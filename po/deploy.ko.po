"FILE_PREVIEW_MAX_SIZE = 30 * 1024 * 1024\n"
"\n"
"# 초 단위 쿠키 수명(기본: 2주).\n"
"SESSION_COOKIE_AGE = 60 * 60 * 24 * 7 * 2\n"
"\n"
"# 모든 요청에 대한 세션 데이터 저장 여부.\n"
"SESSION_SAVE_EVERY_REQUEST = False\n"
"\n"
"# 웹 브라우저를 닫았을 때 사용자 세션 쿠기 유지 여부.\n"
"SESSION_EXPIRE_AT_BROWSER_CLOSE = False\n"
